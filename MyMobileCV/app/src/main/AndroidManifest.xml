<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mobilecv.csinc.com.mobilecv" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="mobilecv.csinc.com.mobilecv.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="mobilecv.csinc.com.mobilecv.AboutMe"
            android:label="@string/title_activity_about_me" >
        </activity>
        <activity
            android:name="mobilecv.csinc.com.mobilecv.Projects"
            android:label="@string/title_activity_projects" >
        </activity>
        <activity
            android:name="mobilecv.csinc.com.mobilecv.Contact"
            android:label="@string/title_activity_contact" >
        </activity>
        <activity
            android:name="mobilecv.csinc.com.mobilecv.OnlineCV"
            android:label="@string/title_activity_online_cv" >
        </activity>
    </application>

</manifest>
